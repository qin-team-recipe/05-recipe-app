{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run predev && next dev",
    "predev": "rm -rf .next",
    "build": "next build",
    "start": "next start",
    "eslint": "eslint --ext .jsx,.js,.tsx,.tsx ./src --fix",
    "lint": "run-p --continue-on-error lint:*",
    "lint:eslint": "eslint --ext .jsx,.js,.tsx,.tsx ./src",
    "lint:prettier": "prettier --write .",
    "fix": "run-s --continue-on-error fix:eslint fix:prettier",
    "fix:eslint": "eslint --ext .jsx,.js,.tsx,.tsx ./src --fix",
    "fix:prettier": "prettier --write ./src",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run fix:eslint",
      "npm run fix:prettier"
    ]
  },
  "dependencies": {
    "@radix-ui/react-scroll-area": "^1.0.4",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-access": "^1.1.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys-custom-order": "^1.0.5",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwind-variants": "^0.1.5",
    "tailwindcss": "^3.3.2",
    "tailwindcss-radix-colors": "^0.3.1",
    "typescript": "^5.1.3"
  },
  "node-version": "18.16.0"
}
