{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "cSpell.words": [
    "darkroup",
    "graydark",
    "tomatodark"
  ]
}
